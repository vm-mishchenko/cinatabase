{
  "name": "cinatabase",
  "version": "0.0.1",
  "description": "Browser local database",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "lint": "tslint -p tsconfig.json",
    "pre-commit": "npm run lint && npm run test",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "bump": "npm version patch",
    "prepublishOnly": "npm run build && npm test && npm run lint",
    "release": "npm run bump && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vm-mishchenko/cinatabase.git"
  },
  "author": "Vitalii Mishchenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vm-mishchenko/cinatabase/issues"
  },
  "homepage": "https://github.com/vm-mishchenko/cinatabase#readme",
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testRegex": "/src/.*\\.spec.(ts)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/playground/"
    ]
  },
  "dependencies": {
    "inversify": "^5.0.1",
    "rxjs": "^6.5.2",
    "reflect-metadata": "^0.1.13",
    "@datorama/akita": "^4.8.1",
    "pouchdb": "^7.0.0",
    "pouchdb-find": "^7.0.0"
  },
  "devDependencies": {
    "@types/pouchdb": "^6.4.0",
    "@types/jest": "^24.0.15",
    "@types/pouchdb-find": "^6.3.4",
    "jest": "^24.8.0",
    "ts-jest": "24.0.0",
    "typescript": "3.5.3",
    "@types/node": "^12.6.8",
    "tslint": "^5.18.0"
  },
  "peerDependencies": {
    "inversify": "^5.0.1",
    "rxjs": "^6.5.2",
    "reflect-metadata": "^0.1.13",
    "@datorama/akita": "^4.8.1",
    "pouchdb": "^7.0.0",
    "pouchdb-find": "^7.0.0"
  }
}
