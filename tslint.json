{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "max-line-length": [
      true,
      120
    ],
    "no-console": [
      true,
      "time",
      "timeEnd",
      "trace"
    ],
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      false,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "ban-keywords",
      "check-format"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "ban-types": false,
    "max-classes-per-file": false
  }
}
